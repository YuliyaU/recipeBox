{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "A Starter Kit for a development in React",
  "main": "index.js",
  "scripts": {
    "watch-webpack": "webpack -w",
    "launch-server": "node server.js",
    "localtunnel": "lt --port 3000 --subdomain yuliyadev",
    "start": "npm-run-all --parallel watch-webpack launch-server start-mock-api lint:watch",
    "share": "npm-run-all --parallel watch-webpack launch-server localtunnel",
    "generate-mock-data": "node buildScripts/generateMockData.js",
    "prestart-mock-api": "npm run generate-mock-data",
    "start-mock-api": "json-server --watch src/api/db.json --port 3001",
    "lint": "node_modules/.bin/esw webpack.config.* src buildScripts",
    "lint:watch": "npm run lint -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YuliyaU/react-starter-kit.git"
  },
  "keywords": [
    "JavaScript",
    "React"
  ],
  "author": "Yuliya Ulanova",
  "bugs": {
    "url": "https://github.com/YuliyaU/react-starter-kit/issues"
  },
  "homepage": "https://github.com/YuliyaU/react-starter-kit#readme",
  "dependencies": {
    "express": "^4.16.2",
    "marked": "^0.3.12",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.9",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-watch": "^3.1.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "json-schema-faker": "^0.5.0-rc13",
    "json-server": "^0.12.1",
    "localtunnel": "^1.8.3",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0"
  }
}
